    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: main.py --config-name epcor_tsdb_scrubber",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "epcor_tsdb_scrubber"
            ],
            "env": {
                "NUMBA_DISABLE_JIT": "1"
            }
        },
        {
            "name": "Python Debugger: main.py --config-name pendulum",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "pendulum"
            ],
            "env": {
                "NUMBA_DISABLE_JIT": "1"
            }
        },
        {
            "name": "Python Debugger: main.py --config-name saturation",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "saturation"
            ]
        },
        {
            "name": "Python Debugger: main.py --config-name mountain_car_continuous",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "mountain_car_continuous",
            ]
        },
        {
            "name": "python e2e/make_configs.py --config-name opc_mountain_car_continuous",
            "type": "debugpy",
            "request": "launch",
            "program": "e2e/make_configs.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "opc_mountain_car_continuous",
            ]
        },
        {
            "name": "python main.py --config-name opc_mountain_car_continuous",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "--config-name",
                "opc_mountain_car_continuous",
            ],
            "env": {
                "NUMBA_DISABLE_JIT": "1"
            }
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ],
            "env": {
                "NUMBA_DISABLE_JIT": "1"
            }
        }
    ]
}
